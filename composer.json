{
    "name": "prone987/ldap-bundle",
    "type": "symfony-bundle",
    "description": "authentication services based on LDAP directories for Symfony 3.4",
    "keywords": ["ldap", "authentication"],
    "license": "MIT",
    "homepage": "https://github.com/Maks3w/FR3DLdapBundle",
    "authors": [
        {
            "name": "Manuhiti AMIOT",
            "homepage": "https://github.com/"
        }
    ]
    },
    "require": {
        "php": ">=7.1",
        "psr/log": "~1.0",
        "symfony/config": "3.4 - 4",
        "symfony/dependency-injection": "3.4 - 4",
        "symfony/security": "3.4 - 4",
        "symfony/security-bundle": "3.4 - 4",
        "zendframework/zend-ldap": "2.5 - 3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
